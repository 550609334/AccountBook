<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DateTextView">
        <attr name="date_type">
            <enum name="year" value="0" />
            <enum name="month" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="CircleText">
        <attr name="circleText" format="string" />
        <attr name="circleColor" format="color" />
        <attr name="circleTextSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ProgressButton">
        <attr name="btn_background" format="color" />
        <attr name="btn_text" format="string" />
        <attr name="btn_textSize" format="dimension" />
    </declare-styleable>
</resources>